		"brand": {
			"logo": {
				"src": "images/logos/icon-simple.svg",
				"background2": "var(--primary-color)",
				"border": "rounded",
				"height": 30
			},

			"logotype": {
				"font": "Varela Round",

				"names": {
					"R": {
						"font_size": "125%",
						"color": "var(--primary-color)"
					},
					"ocket": {
						"color": "var(--primary-color)"
					},
					"K": {
						"font_size": "125%",
						"color": "#a0a0c0"
					},
					"itty": {
						"color": "#a0a0c0"
					}
				}
			}
		},

		"nav": {
			"About": {
				"icon": "fa fa-info-circle",
				"href": "#about"
			},
			"Chats": {
				"icon": "fa fa-message",
