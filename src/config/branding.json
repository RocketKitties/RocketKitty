		"buttons": {
			"show_theme": true
		}
	},

	"welcome": {
		"background": "linear-gradient(to bottom, hsla(200, 90%, 66%, 0%) 0%, hsla(200, 90%, 66%, 50%) 100%)",
		"template": "templates/welcome.tpl",

		"carousel": {
			"template": "templates/welcome/carousel.tpl",

			"scroller": {
				"speed": 0.05,
				"images": {
					"stars": {
						"class": "stars",
						"src": "images/scroller/stars.svg",
						"width": 0.5,
						"height": 0.5,
						"top": 0,
						"left": 0
					},
					"sun": {
						"class": "sun",
						"src": "images/scroller/sunrise.svg",
						"width": 1,
						"top": 0,
						"left": 0
					},
					"moon": {
						"class": "moon",
						"src": "images/scroller/moonrise.svg",
						"width": 1,
						"top": 0,
						"left": 0
					},
					"cloud1": {
						"class": "cloud",
						"src": "images/scroller/clouds.svg",
						"width": 0.5,
						"top": 0.075,
						"left": 0,
						"speed": 0.25
					},
					"cloud2": {
						"class": "cloud",
						"src": "images/scroller/clouds.svg",
						"width": 0.75,
						"top": 0.1,
						"left": 0,
						"speed": -0.33
					},
					"cloud3": {
						"class": "cloud",
						"src": "images/scroller/clouds.svg",
						"width": 0.75,
						"bottom": 0.4,
						"left": 0.33, 
						"speed": 0.5
					},
					"cloud4": {
						"class": "cloud",
						"src": "images/scroller/clouds.svg",
						"width": 1,
						"bottom": 0.15,
						"left": 0,
						"speed": -0.75
					},
					"cloud5": {
						"class": "cloud",
						"src": "images/scroller/clouds2.svg",
						"width": 1.25,
						"bottom": -0.2,
						"left": 0.5, 
						"speed": 1
					},
					"jet": {
						"class": "jet",
						"src": "images/scroller/flying-jet.svg",
						"width": 1,
						"skip": 15,
						"top": 0,
						"left": -2,
						"speed": -5
					},
					"kite": {
						"class": "kite",
						"src": "images/scroller/flying-kite.svg",
						"width": 1,
						"skip": 2,
						"top": 0.25,
						"left": 0,
						"speed": 1
					},
					"ufo": {
						"class": "ufo",
						"src": "images/scroller/flying-ufo.svg",
						"width": 1,
						"skip": 20,
						"top": 0.8,
						"left": 0,
						"speed": 2
					}
				}
			}
		},

		"splash": {
			"background": "transparent",

			"brand": {
				"logo": {
					"src": "images/logos/icon.svg",
					"background2": "var(--primary-color)",
