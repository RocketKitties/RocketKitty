				"color": "hsl(90, 50%, 60%)"
			},
			"Photos": {
				"icon": "fa fa-image",
				"href": "http://photokitty.org",
				"platform": "desktop-only",
				"color": "hsl(40, 90%, 60%)"
			},
			"Code": {
				"icon": "fa fa-code",
				"href": "http://sourcekitty.org",
				"platform": "desktop-only",
				"color": "hsl(25, 100%, 66%)"
			},
			"Files": {
				"icon": "fa fa-file",
				"href": "http://filekitty.org",
				"platform": "desktop-only",
				"color": "hsl(30, 25%, 50%)"
			},
			"S3": {
				"icon": "fa fa-bucket",
				"href": "http://s3kitty.org",
				"platform": "desktop-only",
				"color": "hsl(0, 100%, 75%)"
			},
			"Dreams": {
				"icon": "fa fa-moon",
				"href": "http://dreamkitty.org",
				"platform": "desktop-only",
				"color": "hsl(300, 33%, 50%)"
			},
			"Music": {
				"icon": "fa fa-music",
				"href": "http://musickitty.org",
				"platform": "desktop-only",
				"color": "hsl(250, 33%, 60%)"
