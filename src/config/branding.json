						"K": {
							"font_size": "150%",
							"color": "#a0a0c0"
						},
						"itty": {
							"color": "#a0a0c0"
						},
						".org": {
							"font_size": "25%"
						}
					}
				}
			},

			"tagline": {
				"text": "Launch Your Own Kitty!",
				"font": "Syncopate",
				"font_weight": "bold",
				"color": "white",
				"background_color": "hsl(-10, 90%, 66%)",
				"border": "rounded"
			},

			"description": {
				"text": "Family of free, friendly, self-hostable cloud-based sharing platforms."
			}
		}
